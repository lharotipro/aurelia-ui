{
  "name": "@gre/ui-components-aurelia",
  "description": "GRE Aurelia UI Components Library: A collection of customizable UI components including tables, selects, loaders ... , and authorization management tools",
  "version": "1.0.0",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@popperjs/core": "^2.11.8",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.4.0",
    "aurelia-cli": "^3.0.1",
    "aurelia-configuration": "^2.0.0",
    "aurelia-dependency-injection": "^1.5.2",
    "aurelia-event-aggregator": "^1.0.3",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-testing": "^1.1.0",
    "aurelia-validation": "^2.0.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "bootstrap-icons-font": "^2.0.7",
    "connect": "^3.7.0",
    "connect-history-api-fallback": "^2.0.0",
    "connect-injector": "^0.4.4",
    "core-js": "^3.37.1",
    "dayjs": "^1.11.11",
    "debounce": "^1.2.1",
    "del": "^6.1.0",
    "eslint": "^8.43.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-eslint-new": "^1.8.1",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-watch": "^5.0.1",
    "minimatch": "^5.1.2",
    "open": "^8.4.0",
    "promise-polyfill": "^8.3.0",
    "requirejs": "^2.3.6",
    "sass": "^1.77.4",
    "serve-static": "^1.15.0",
    "server-destroy": "^1.0.1",
    "socket.io": "^4.7.1",
    "text": "requirejs/text"
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "sass:main": "sass src/common.scss src/common.css",
    "sass:input-datepicker": "sass src/elements/input-datepicker/input-datepicker.scss src/elements/input-datepicker/input-datepicker.css",
    "sass:date-filter": "sass src/elements/table/filter/filter-type-column/date-filter.scss src/elements/table/filter/filter-type-column/date-filter.css",
    "sass:lock": "sass src/elements/lock/lock.scss src/elements/lock/lock.css",
    "sass:select": "sass src/elements/select/select.scss src/elements/select/select.css",
    "sass:auto-complete": "sass src/elements/auto-complete/auto-complete.scss src/elements/auto-complete/auto-complete.css",
    "sass": "npm run sass:input-datepicker & npm run sass:date-filter & npm run sass:lock & npm run sass:select & npm run sass:auto-complete & npm run sass:main",
    "build": "npm run sass && au build-plugin --env prod",
    "start": "au run",
    "watch:sass": "npm run watch:sass:input-datepicker & npm run watch:sass:date-filter & npm run watch:sass:lock  & npm run watch:sass:select & npm run watch:sass:auto-complete & npm run watch:sass:main",
    "watch:sass:input-datepicker": "sass --watch src/elements/input-datepicker/input-datepicker.scss:src/elements/input-datepicker/input-datepicker.css",
    "watch:sass:date-filter": "sass --watch src/elements/table/filter/filter-type-column/date-filter.scss:src/elements/table/filter/filter-type-column/date-filter.css",
    "watch:sass:lock": "sass --watch src/elements/lock/lock.scss:src/elements/lock/lock.css",
    "watch:sass:select": "sass --watch src/elements/select/select.scss:src/elements/select/select.css",
    "watch:sass:auto-complete": "sass --watch src/elements/auto-complete/auto-complete.scss:src/elements/auto-complete/auto-complete.css",
    "watch:sass:main": "sass --watch src/common.scss:src/common.css",
    "watch": "npm run watch:sass & npm run au:watch",
    "au:watch": "au build-plugin --watch",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "overrides": {
    "chokidar": "^3.0.0",
    "glob-stream": "^7.0.0",
    "glob-parent": "^6.0.0",
    "micromatch": "^4.0.0"
  },
  "main": "dist/commonjs/index.js",
  "module": "dist/native-modules/index.js",
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "vanillajs-datepicker": "^1.3.4"
  }
}
